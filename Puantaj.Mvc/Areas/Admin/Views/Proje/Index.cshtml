@model ProjeListDto
<div class="container-fluid pt-4 px-4">
    <div class="row vh-100 bg-secondary rounded align-items-center justify-content-center mx-0">
        <div class="col-md-6 text-center">
            <table id="proje" class="table table-striped table-bordered" style="width:100%">
                <thead>
                    <tr>
                        <th>Proje Adı</th>
                        <th>Hizmet Turu</th>
                        <th>Kurum</th>
                        <th>Eklenme Tarihi</th>
                        <th>Durumu</th>
                        <th>Segment</th>
                        <th>Segment Yöneticisi</th>
                        <th>GMY</th>
                        <th>SFM Sorumlusu</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.Projes)
                    {
                        <tr>
                            <td>@item.ProjeAdi</td>
                            <td>@item.HizmetTuru</td>
                            <td>@item.Kurum</td>
                            <td>@item.EklemeTarihiGun/@item.EklemeTarihiAy/@item.EklemeTarihiYil</td>
                            <td>@item.Durumu</td>
                            <td>@item.Segment</td>
                            <td>@item.SegmentYoneticisi</td>
                            <td>@item.Gmy</td>
                            <td>@item.Sfmsorumlusu</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>

@section DataTable
    {
    <script src="https://cdn.datatables.net/1.13.1/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.13.1/js/dataTables.bootstrap4.min.js"></script>
    <script>
      
    </script>
}

@section DataTableStyle
    {
    <style>
        #proje_length {
            display: none
        }

        #proje_filter {
            margin-bottom: 30px
        }

        #proje_paginate {
            margin-top: 30px
        }
    </style>
}